{
    "snapshotSerializers":[
        "enzyme-to-json/serializer"
    ],
    "setupFiles":[
        "raf/polyfill.js",
        "<rootDir>/src/tests/setupTests.js"
    ],
    "moduleNameMapper": {
        "^.+\\.(css|less|scss)$": "<rootDir>/src/tests/__mocks__/styleMock.js",
        "^.+\\.(jpg|png|gif|svg)$": "identity-obj-proxy"
    },
    "verbose": true,
    "testURL": "http://localhost/"
}
